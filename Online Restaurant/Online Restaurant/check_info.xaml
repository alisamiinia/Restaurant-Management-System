<Window x:Class="Online_Restaurant.check_info"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Online_Restaurant"
        mc:Ignorable="d"
        Title="بررسی اطلاعات امنیتی" Height="200" Width="300">
    <StackPanel>
        <TextBlock Text=" Username :" FontSize="20"/>
        <TextBox Name="username" Margin="5 0 5 15"/>
        <TextBlock Text=" Password :" FontSize="20"/>
        <TextBox Name="password" Margin="5 0 5 15"/>
        <Button Content="ورود" Click="Button_Click" Margin="100 10"/>
    </StackPanel>
</Window>